MDBoxLayout:
    orientation: "vertical"
    MDToolbar:
        title: "Audio"
        md_bg_color: .2, .2, .2, 1
        left_action_items: [["assets/vlc.png", lambda x: app.callback()]]
        right_action_items: [["text-search", lambda x: app.callback_1()], ["replay", lambda x: app.callback_2()], ["dots-vertical", lambda x: app.callback_3()]]
    MDTabs:
        id: tabs
        on_tab_switch: app.on_tab_switch(*args)
        indicator_color: app.theme_cls.accent_color
        background_color: .2, .2, .2, 1
        text_color_active: app.theme_cls.accent_color
        Tab:
            title: "ALBUMS"
        Tab:
            title: "GENRE"
        Tab:
            title: "ARTITES"
        Tab:
            title: "PISTES"
        Tab:
            title: "Tab 2"
            


